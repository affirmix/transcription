mixin parameters(method)
  - var params = method.comments.tags.filter(function(element){return element.title == "param"})
  each param, index in params
    if index > 0
      | ,&nbsp;
    a(href="#method-" + method.name + "-param-" + param.name)= param.name

html
  head
  body
    dl
    each file in files
      each classObject in file.classes
        dt(class=classObject.isPublic ? "public" : "", class=classObject.isDefault ? "default" : "")= classObject.name
        each method in classObject.methods
          dd
            a(href="#method-" + method.name)= method.name
            | (
            +parameters(method)
            | )
